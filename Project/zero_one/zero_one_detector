:vpi_module "vhdl_sys";
:vpi_module "v2005_math";
:vpi_module "va_math";
S_0x55fb69f4b2d0 .scope module, "test_zero_one" "test_zero_one" 2 3;
 .timescale -9 -12;
v0x55fb69f6bc90_0 .var "A", 0 0;
v0x55fb69f6bd50_0 .net "Y", 0 0, L_0x55fb69f6c020;  1 drivers
v0x55fb69f6be20_0 .var "clk", 0 0;
v0x55fb69f6bf20_0 .var "rst", 0 0;
S_0x55fb69f4b450 .scope module, "DUT" "zero_one_detector" 2 7, 3 3 0, S_0x55fb69f4b2d0;
 .timescale -9 -12;
    .port_info 0 /INPUT 1 "A"
    .port_info 1 /INPUT 1 "clk"
    .port_info 2 /INPUT 1 "rst"
