P_0x55fb69f4b5d0 .param/l "S0" 0 3 6, C4<00>;
P_0x55fb69f4b610 .param/l "S1" 0 3 7, C4<01>;
P_0x55fb69f4b650 .param/l "S2" 0 3 8, C4<10>;
v0x55fb69f4b790_0 .net "A", 0 0, v0x55fb69f6bc90_0;  1 drivers
v0x55fb69f6b6d0_0 .net "Y", 0 0, L_0x55fb69f6c020;  alias, 1 drivers
L_0x7fdd6dfdf018 .functor BUFT 1, C4<10>, C4<0>, C4<0>, C4<0>;
v0x55fb69f6b790_0 .net/2u *"_s0", 1 0, L_0x7fdd6dfdf018;  1 drivers
v0x55fb69f6b880_0 .net "clk", 0 0, v0x55fb69f6be20_0;  1 drivers
v0x55fb69f6b940_0 .var "nextstate", 1 0;
v0x55fb69f6ba70_0 .net "rst", 0 0, v0x55fb69f6bf20_0;  1 drivers
v0x55fb69f6bb30_0 .var "state", 1 0;
E_0x55fb69f3e670 .event edge, v0x55fb69f6bb30_0, v0x55fb69f4b790_0;
E_0x55fb69f3e7a0 .event posedge, v0x55fb69f6ba70_0, v0x55fb69f6b880_0;
L_0x55fb69f6c020 .cmp/eq 2, v0x55fb69f6bb30_0, L_0x7fdd6dfdf018;
    .scope S_0x55fb69f4b450;
T_0 ;
    %wait E_0x55fb69f3e7a0;
    %load/vec4 v0x55fb69f6ba70_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.0, 8;
    %pushi/vec4 0, 0, 2;
    %assign/vec4 v0x55fb69f6bb30_0, 0;
    %jmp T_0.1;
T_0.0 ;
    %load/vec4 v0x55fb69f6b940_0;
    %assign/vec4 v0x55fb69f6bb30_0, 0;
T_0.1 ;
    %jmp T_0;
    .thread T_0;
    .scope S_0x55fb69f4b450;
T_1 ;
    %wait E_0x55fb69f3e670;
    %load/vec4 v0x55fb69f6bb30_0;
    %dup/vec4;
    %pushi/vec4 0, 0, 2;
    %cmp/u;
    %jmp/1 T_1.0, 6;
    %dup/vec4;
    %pushi/vec4 1, 0, 2;
    %cmp/u;
    %jmp/1 T_1.1, 6;
    %dup/vec4;
    %pushi/vec4 2, 0, 2;
    %cmp/u;
    %jmp/1 T_1.2, 6;
    %pushi/vec4 0, 0, 2;
    %store/vec4 v0x55fb69f6b940_0, 0, 2;
    %jmp T_1.4;
T_1.0 ;
    %load/vec4 v0x55fb69f4b790_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_1.5, 8;
    %pushi/vec4 0, 0, 2;
    %store/vec4 v0x55fb69f6b940_0, 0, 2;
    %jmp T_1.6;
T_1.5 ;
    %pushi/vec4 1, 0, 2;
    %store/vec4 v0x55fb69f6b940_0, 0, 2;
T_1.6 ;
    %jmp T_1.4;
T_1.1 ;
    %load/vec4 v0x55fb69f4b790_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_1.7, 8;
    %pushi/vec4 2, 0, 2;
    %store/vec4 v0x55fb69f6b940_0, 0, 2;
    %jmp T_1.8;
T_1.7 ;
    %pushi/vec4 1, 0, 2;
    %store/vec4 v0x55fb69f6b940_0, 0, 2;
T_1.8 ;
    %jmp T_1.4;
T_1.2 ;
    %load/vec4 v0x55fb69f4b790_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_1.9, 8;
    %pushi/vec4 0, 0, 2;
    %store/vec4 v0x55fb69f6b940_0, 0, 2;
    %jmp T_1.10;
T_1.9 ;
    %pushi/vec4 1, 0, 2;
    %store/vec4 v0x55fb69f6b940_0, 0, 2;
T_1.10 ;
    %jmp T_1.4;
T_1.4 ;
    %pop/vec4 1;
    %jmp T_1;
    .thread T_1, $push;
    .scope S_0x55fb69f4b2d0;
T_2 ;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x55fb69f6be20_0, 0, 1;
    %delay 5000, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x55fb69f6be20_0, 0, 1;
    %delay 5000, 0;
    %jmp T_2;
    .thread T_2;
    .scope S_0x55fb69f4b2d0;
T_3 ;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x55fb69f6bf20_0, 0, 1;
    %delay 10000, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x55fb69f6bf20_0, 0, 1;
    %end;
    .thread T_3;
    .scope S_0x55fb69f4b2d0;
T_4 ;
    %vpi_call 2 23 "$dumpfile", "test01.vcd" {0 0 0};
    %vpi_call 2 24 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x55fb69f4b450 {0 0 0};
